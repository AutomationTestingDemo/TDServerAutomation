    "messageType": {
      "description": "Message Type",
      "type": "string"
    },
    "threeDSMethodURL": {
      "description": "TDS method URL",
      "type": "string",
      "maxLength":256,
      "minLength":1
    },
    "messageVersion": {
      "description": "Message Version",
      "type": "string",
      "maxLength":8,
      "minLength":5
    },
    "callerTxnRefID": {
      "description": "Caller Transaction Ref ID",
      "type": "string",
      "maxLength":36,
      "minLength":0
    },
    "threeDSServerTransID": {
      "description": "TDS Server Trans ID",
      "type": "string",
        "maxLength":36,
      "minLength":36
